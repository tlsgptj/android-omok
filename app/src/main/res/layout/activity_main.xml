<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/start_game_text"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:text="게임을 시작해주세요."
        android:textStyle="bold"
        android:gravity="center"
        android:textSize="30dp"
        android:layout_marginTop="80dp"
        android:visibility="visible" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:text="Omok"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:textSize="50dp"
        android:background="#9633FF"/>

    <TextView
        android:id="@+id/winnerText"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="80dp"
        android:background="#F79D44"
        android:gravity="center"
        android:textStyle="bold"
        android:text="@string/winner_text"
        android:textAlignment="center"
        android:textColor="#795548"
        android:textSize="30dp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/intheDol"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="80dp"
        android:background="#F79D44"
        android:gravity="center"
        android:text="돌이 있는 위치입니다!"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#795548"
        android:textSize="30dp"
        android:visibility="visible" />


    <ImageView
        android:id="@+id/black_stone"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:background="@drawable/blackdol"/>

    <ImageView
        android:id="@+id/white_stone"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:background="@drawable/whitedol"/>

